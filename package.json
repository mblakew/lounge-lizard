{
  "name": "lounge-lizard",
  "version": "0.4.0",
  "main": "lib/main.js",
  "description": "Fast open source multi-service desktop chat application",
  "build": {
    "appId": "org.cacti-council.lounge-lizard",
    "productName": "Lounge Lizard",
    "copyright": "Copyright © 2018 Cheng Zhao, Copyright © 2019-2020 Cacti Council",
    "unpack": "+(*.node|*.html|*.png|*.gif)"
  },
  "bin": {
    "lounge-lizard": "./scripts/start.js"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "install": "fetch-yode",
    "build": "yackage build out --cache-dir yode",
    "dist": "yackage dist out --cache-dir yode"
  },
  "engines": {
    "node": "^10",
    "yode": "0.4.2"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/cacticouncil/lounge-lizard",
  "repository": {
    "type": "git",
    "url": "https://github.com/cacticouncil/lounge-lizard.git"
  },
  "bugs": {
    "url": "https://github.com/cacticouncil/lounge-lizard/issues"
  },
  "dependencies": {
    "@slack/client": "4.8.0",
    "axios": "^0.19.2",
    "binary-search-bounds": "2.0.4",
    "download-yue": "2.x",
    "fetch-yode": "0.2.0",
    "fs-extra": "5.0.0",
    "gui": "0.6.3",
    "html-to-mrkdwn": "^3.0.0",
    "keytar": "4.3.0",
    "mini-signals": "1.1.1",
    "node-notifier": "^6.0.0",
    "opn": "5.3.0",
    "qs": "^6.9.4",
    "readable-size": "0.2.0",
    "request": "^2.88.0",
    "sanitize-html": "1.20.0",
    "slackify-markdown": "^2.1.0",
    "string-replace-async": "1.2.1"
  },
  "devDependencies": {
    "yackage": "0.2.10"
  }
}
